<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300" prefWidth="500" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cse213.final_project.sadika.approveRejectApplicationController">
    <Label layoutX="20" layoutY="20" text="Final Verification Result" />
    <TextField fx:id="applicationIdField" layoutX="20" layoutY="60" promptText="Application ID" />
    <Button layoutX="20" layoutY="110" onAction="#approveOnAction" text="Approve" />
    <Button layoutX="150" layoutY="110" onAction="#rejectOnAction" text="Reject" />
   <Button layoutX="30.0" layoutY="225.0" mnemonicParsing="false" onAction="#backOnAction" text="Back" />
</AnchorPane>
